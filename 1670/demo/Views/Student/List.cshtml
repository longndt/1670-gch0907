@model List<Student>


<div class="card-group">
    @foreach (var student in Model)
   {
        <div class="card col-md-3">
                <img src="@student.Image" width="200" height="200">
                <h3>@student.Name</h3>
                <h4>@student.Email</h4>
        </div>
   } 
</div>
